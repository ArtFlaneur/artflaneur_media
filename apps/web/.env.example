# Sanity Configuration
# Copy this file to .env.local and fill in your values
# Get these values from https://www.sanity.io/manage
VITE_SANITY_PROJECT_ID=your-project-id
VITE_SANITY_DATASET=production
VITE_SANITY_API_VERSION=2024-01-01
VITE_SANITY_STUDIO_URL=http://localhost:3333

# Directus Configuration
# AWS Directus instance for galleries, exhibitions, and artists data
VITE_DIRECTUS_URL=https://your-directus-instance.amazonaws.com
VITE_DIRECTUS_TOKEN=your-directus-api-token
